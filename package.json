      "php-refactoring:rename-variable",
      "php-refactoring:convert-variable",
      "php-refactoring:optimize"
    ]
  },
  "repository": "https://github.com/mihaliak/php-refactoring",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0"
  }
}
