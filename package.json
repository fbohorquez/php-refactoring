{
  "name": "php-refactoring",
  "main": "./lib/php-refactoring",
  "version": "2.0.0",
  "description": "PHP Refactoring Browser implementation for atom.",
  "keywords": [
    "php",
    "refactoring",
    "extract",
    "method",
    "convert",
    "optimize",
    "variable"
